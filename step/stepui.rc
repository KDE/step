<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE gui SYSTEM "kpartgui.dtd">
<gui version="1" name="step" translationDomain="step">
  <ToolBar name="mainToolBar" >
    <text>Main Toolbar</text>
    <Action name="view_zoom_in" />
    <Action name="view_zoom_out" />
  </ToolBar>
  <ToolBar name="simulationToolBar" >
    <text>Simulation Toolbar</text>
    <Action name="simulation_start_stop" />
    <Action name="run_speed" />

  </ToolBar>
  <MenuBar>
    <Menu name="file" >
      <Action name="file_tutorial_open" />
      <Menu name="examples">
        <text>&amp;Examples</text>
        <Action name="file_example_open" />
        <Action name="file_example_openlocal" />
        <Action name="file_example_download" />
        <Action name="file_example_upload" />
      </Menu>
    </Menu>
    <Menu name="edit" >
      <Action name="edit_delete" />
    </Menu>
    <Menu name="simulation" >
      <text>&amp;Simulation</text>
      <Action name="simulation_start" />
      <Action name="simulation_stop" />
        <Menu name="speed"><text>&amp;Run Speed</text>
          <Action name="full_speed" />
          <Action name="slow_speed" />
          <Action name="slower_speed" />
          <Action name="slowest_speed" />
          <Action name="step_speed" />
        </Menu>
    </Menu>
    <Menu name="settings" >
        <Menu name="panels" >
            <text>&amp;Panels</text>
            <Action name="toggle_palette_dock" />
            <Action name="toggle_world_dock" />
            <Action name="toggle_properties_dock" />
            <Action name="toggle_info_dock" />
            <Action name="toggle_undo_dock" />
        </Menu>
    </Menu>
  </MenuBar>
</gui>
